@base <https://raw.githubusercontent.com/energy-domain/ontology-tests/main/001/02_specialized/> .
@prefix : <> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix energyBase: <https://raw.githubusercontent.com/energy-domain/ontology-tests/main/001/01_base/> .

##################################################
### Base ontologies imports ###
##################################################
#<> owl:imports <https://raw.githubusercontent.com/energy-domain/ontology-tests/main/001/01_base/01_attributes_definition.ttl> .

##################################################
### Assets Specialized Attributes ###
##################################################
energyBase:Equipment owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        [ a owl:Restriction ;
          owl:onProperty energyBase:hasAttribute ;
          owl:someValuesFrom energyBase:SerialNumber ;
          owl:cardinality 1 ]
        [ a owl:Restriction ;
          owl:onProperty energyBase:hasAttribute ;
          owl:someValuesFrom energyBase:MaintenanceHistory ;
          owl:minCardinality 1 ]
    )
] .

energyBase:Pump owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        [ a owl:Restriction ;
          owl:onProperty energyBase:hasAttribute ;
          owl:someValuesFrom energyBase:Pressure ;
          owl:cardinality 1 ]
        [ a owl:Restriction ;
          owl:onProperty energyBase:hasAttribute ;
          owl:someValuesFrom energyBase:Temperature ;
          owl:cardinality 1 ]
    )
] .

energyBase:Tank owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty energyBase:hasAttribute ;
    owl:someValuesFrom energyBase:Volume ;
    owl:cardinality 1 
] .
